{
  "spaces": [
    {
      "name": "default",
      "retentionPolicy": "1d",
      "shardDuration": "1h",
      "regex": "/.*/",
      "replicationFactor": 1,
      "split": 1
    },
    {
      "name": "1_hour_rollup",
      "retentionPolicy": "7d",
      "shardDuration": "1d",
      "regex": "/^1h.*/",
      "replicationFactor": 1,
      "split": 1
    },
    {
      "name": "1_day_rollup",
      "retentionPolicy": "728d",
      "shardDuration": "7d",
      "regex": "/^1d.*/",
      "replicationFactor": 1,
      "split": 1
    }
  ],
  "continuousQueries": [
    "select mean(value) from /^[a-z]+.*$/ group by time(1h) into 1h.:series_name",
    "select mean(value) from /^[a-z]+.*$/ group by time(1d) into 1d.:series_name"
  ]
}